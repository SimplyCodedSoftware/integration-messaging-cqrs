{
    "name": "simplycodedsoftware/integration-messaging-cqrs",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Dariusz Gafka",
            "email": "dgafka.mail@gmail.com"
        }
    ],
    "keywords": ["messaging", "message", "spring", "enterprise integration patterns", "cqrs", "command", "query", "separation", "handler", "bus"],
    "description": "Provides cqrs on top of integration messaging",
    "autoload": {
        "psr-4": {
            "SimplyCodedSoftware\\IntegrationMessaging\\Cqrs\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\SimplyCodedSoftware\\IntegrationMessaging\\Http\\": "tests/phpunit",
            "Behat\\Bootstrap\\": "tests/Behat/Bootstrap",
            "Fixture\\": "tests/Fixture"
        }
    },
    "require": {
        "php": ">=7.1.0",
        "simplycodedsoftware/integration-messaging": "^0.4.0",
        "doctrine/orm": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2.3",
        "behat/behat": "^3.3.1",
        "phan/phan": "^0.10.0"
    },
    "config": {
        "bin-dir": "bin/"
    }
}
